<% include ../partials/header %>
    <main>
        <h1><%= currentUser.username %>'s polls</h1>

        <section class="polls">
            <ul>
                <% polls.forEach((poll) => { %>
                    <a href="/polls/<%= poll._id %>"><li><%= poll.question %></li></a>
                <% }) %>
            </ul>
        </section>
    </main>
<% include ../partials/footer %>